-- 1. Enable Hybrid Custom Mode
getgenv().EnableHybridCustom = true 

-- 2. Define Hybrid Settings
getgenv().HybridSettings = {
    idle1  = "Stylish", -- ‡∏ó‡πà‡∏≤‡∏¢‡∏∑‡∏ô‡∏ô‡∏¥‡πà‡∏á 1
